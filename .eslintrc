{
  "extends": "@folio/eslint-config-stripes/acquisitions",
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "overrides": [
    {
      "files": [ "src/**/tests/*", "test/**/*" ],
      "rules": {
        "@typescript-eslint/triple-slash-reference": "off",
        "react/prop-types": "off"
      }
    }
  ],
  "env": {
    "jest": true
  }
}
